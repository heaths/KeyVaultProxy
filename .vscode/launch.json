{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Tests (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/tests/bin/Debug/netcoreapp2.1/KeyVaultProxy.Tests.dll",
      "args": [],
      "cwd": "${workspaceFolder}/tests",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
